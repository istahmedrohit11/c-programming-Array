#include<stdio.h>
int main(){
    int arr[50],size;
    printf("Enter the size of: ");
    scanf("%d",&size);
    printf("Enter the element of array: ");
    for(int i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    printf("The elements are:");
    for(int i=0;i<size;i++){
        printf("%d ",arr[i]);
    }
    return 0;
}
